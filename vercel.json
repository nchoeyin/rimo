{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist/rimo/browser" } },
    { "src": "dist/rimo/server/server.mjs", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/dist/rimo/server/server.mjs" }
  ],
  "public": true
}

